function createTooltips ()
{


$('.draggable').each(function(){



  $(this).qtip(
  {
    content: $(this).attr('name'),
    position: {
      corner: {
        target: 'topMiddle',
         tooltip: 'bottomMiddle'
      }
    },
    style: { 
      width: 100,
      padding: 5,
      background: '#A2D959',
      color: 'black',
      textAlign: 'center',
      border: {
         width: 1,
         radius: 0,
         color: '#A2D959'
      },
      tip: true,
    }
  });


});

};





$(document).ready(createTooltips);
